90 C=&H100 -1
100 INPUT"OFFSET";OFFS
110 FOR I=0 TO C
120 L=PEEK(OFFS+I)
130 IF (L>=32) THEN PRINT CHR$(L); ELSE GOSUB1000:PRINT "<";A$;">";
140 NEXT
150 END
1000 HI=INT(L/16):LO=L MOD 16
1010 IF HI>9 THEN HI=HI+7
1020 IF LO>9 THEN LO=LO+7
1030 A$=CHR$(HI+48)+CHR$(LO+48)
1040 RETURN
